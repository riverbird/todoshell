{{extend 'layout.html'}}
<h1>List all tasks</h1>
{{=form}}
<table>
{{for task in tasks:}}
<tr>
<td>{{=A{task.subject,_href=URL(r=request,f='show?id=%s'%task.id))}}</td>
<td>{{=task.create_date}}</td>
</tr>
{{pass}}
</table>
{{=A('create new task',_href=URL(r=request,f='new_task'))}}
